<style>
    table, th, td { border: 1px solid black }
</style>

<h1>Students studying <%= mid%></h1>
<table>
    <tr>
        <!-- Table headings -->
        <th>Student ID</th>
        <th>Student Name</th>
        <th>GPA</th>
    </tr>
    <!-- Create a new row for each module -->
    <% students.forEach((student)=> { %>
        <tr>
            <td>
                <%= student.sid %>
            </td>
            <td>
                <%= student.name %>
            </td>
            <td>
                <%= student.gpa %>
            </td>
        </tr>
        <% }) %>
</table>

<a href="http://localhost:3004/">Home</a> <br>